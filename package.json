{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-facebook-import-status": "cd nova-components/FacebookImportStatus && npm run dev",
        "build-facebook-import-status-prod": "cd nova-components/FacebookImportStatus && npm run prod",
        "build-event-overview": "cd nova-components/EventOverview && npm run dev",
        "build-event-overview-prod": "cd nova-components/EventOverview && npm run prod",
        "build-fb-token-overview": "cd nova-components/FbTokenOverview && npm run dev",
        "build-fb-token-overview-prod": "cd nova-components/FbTokenOverview && npm run prod"
    },
    "devDependencies": {
        "algoliasearch": "^4.1.0",
        "alpinejs": "^2.3.3",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^5.1",
        "laravel-mix": "^4.0.7",
        "laravel-mix-purgecss": "^4.2.0",
        "mapbox-gl": "^1.10.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "tailwindcss": "^1.1.3",
        "vue": "^2.6.11",
        "vue-instantsearch": "^2.6.0",
        "vue-template-compiler": "^2.6.11"
    }
}
